
#include<stdio.h>
int main() 
{ 
    int n,i,j,sum=0; 
    scanf("%d",&n); 
    int a[n]; 
    for(i=0;i<n;i++) 
    { 
        scanf("%d",&a[i]); 
    
    } 
    for(i=0;i<n;i++) 
    { 
        if(a[i]==5) 
        { 
            for(j=i+1,j<n;j++) 
            { 
                if(a[i]!=10) 
                { 
                    sum+=a[i];
                } 
                else 
                { 
                    break; 
                }
            } 
            i=j-1;
        }
    }
} 


input: 9 
       2 5 3 2 10 3 10 5 4 
       
output: 9
